  urldate = {2016-01-05},
  date = {2015-05-15},
  pages = {1541--1549},
  keywords = {humoral,IFN-γ,influenza,LAIV,longevity,pediatric,T-cellular},
  file = {Mohn et al. - 2015 - Longevity of B-Cell and T-Cell Responses After Liv.pdf:/home/edwin/Documents/research/zotero/storage/FZUBZCCJ/Mohn et al. - 2015 - Longevity of B-Cell and T-Cell Responses After Liv.pdf:application/pdf;Snapshot:/home/edwin/Documents/research/zotero/storage/WSBWU8QN/1541.html:text/html},
