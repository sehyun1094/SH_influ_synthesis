  pages = {201607747},
  keywords = {empirical dynamic modeling,epidemiology,nonlinear dynamics,physical–biological coupling,state-dependence},
  file = {Deyle et al. - 2016 - Global environmental drivers of influenza.pdf:/home/edwin/Documents/research/zotero/storage/XHXQD4XQ/Deyle et al. - 2016 - Global environmental drivers of influenza.pdf:application/pdf;Snapshot:/home/edwin/Documents/research/zotero/storage/2F56MAER/1607747113.html:text/html},
