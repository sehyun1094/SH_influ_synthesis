  author = {Manchanda, Himanshu and Seidel, Nora and Krumbholz, Andi and Sauerbrei, Andreas and Schmidtke, Michaela and Guthke, Reinhard},
  urldate = {2016-01-13},
  date = {2014-04},
  pages = {51--59},
  keywords = {Immune response,Infection,inflammation,Influenza virus,Modeling,Virus replication},
  file = {Manchanda et al. - 2014 - Within-host influenza dynamics A small-scale math.pdf:/home/edwin/Documents/research/zotero/storage/5B6IUKBW/Manchanda et al. - 2014 - Within-host influenza dynamics A small-scale math.pdf:application/pdf;Manchanda et al. - 2014 - Within-host influenza dynamics A small-scale math.html:/home/edwin/Documents/research/zotero/storage/EVJM879U/Manchanda et al. - 2014 - Within-host influenza dynamics A small-scale math.html:text/html}
