  urldate = {2015-12-11},
  date = {2015},
  pages = {jiv195},
  keywords = {clinical trial,hemagglutinin,immune correlates,influenza,influenza vaccine,neuraminidase,serologic assays,vaccine response},
  file = {Monto et al. - 2015 - Antibody to Influenza Virus Neuraminidase An Inde.pdf:/home/edwin/Documents/research/zotero/storage/VP8GDT6M/Monto et al. - 2015 - Antibody to Influenza Virus Neuraminidase An Inde.pdf:application/pdf;Snapshot:/home/edwin/Documents/research/zotero/storage/4ZQUXWIA/infdis.jiv195.html:text/html},
  eprinttype = {pmid},
